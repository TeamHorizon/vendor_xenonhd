# AOKP Extras

on boot
    # Run sysinit
    start sysinit

# adb over network
on property:service.adb.tcp.port=5555
    stop adbd
    start adbd
on property:service.adb.tcp.port=-1
    stop adbd
    start adbd
    
# Compcache - handle at boot
service compcache /system/bin/handle_compcache
    user root
    group root
    oneshot

# sysinit (/system/etc/init.d)
service sysinit /system/bin/sysinit
    user root
    oneshot
